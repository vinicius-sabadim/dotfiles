alias l='ls -lhaG'
alias ..='cd ..'
alias reload='source ~/bin/dotfiles/bash/aliases'
alias top10='history | awk '"'"'{a[$2]++}END{for(i in a){print a[i] " " i}}'"'"' | sort -rn | head'
alias open='xdg-open .'
alias myip='ifconfig | grep "inet" | sort'
